<?xml version="1.0" encoding="utf-8" ?>
<views:AgeContentPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:views="clr-namespace:AgeCal.Views"
            x:Class="AgeCal.Views.MainPage"
            xmlns:local="clr-namespace:AgeCal"
            xmlns:viewModel="clr-namespace:AgeCal.ViewModels"
             x:TypeArguments="viewModel:MainViewModel">

    <ContentPage.Content>

        <StackLayout x:Name="MainContainer" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" Spacing="0">

            <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                <ActivityIndicator IsRunning="True" IsEnabled="True" Color="{StaticResource  PrimaryGray}" HeightRequest="80" WidthRequest="80" />
            </StackLayout>


        </StackLayout>
    </ContentPage.Content>
</views:AgeContentPage>