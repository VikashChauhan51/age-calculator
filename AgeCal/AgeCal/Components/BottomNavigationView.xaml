<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AgeCal.Components.BottomNavigationView">
    <ContentView.Content>
        <StackLayout x:Name="BottomNavBar" Spacing="0" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" HeightRequest="80" BackgroundColor="{StaticResource PrimaryWhite}">
            <BoxView x:Name="TopDivider"  Style="{StaticResource SeparatorBoxView}" VerticalOptions="Start"   HorizontalOptions="FillAndExpand">

            </BoxView>
            <FlexLayout x:Name="IconLayout" HorizontalOptions="FillAndExpand" AlignItems="Center" HeightRequest="79" JustifyContent="Center"></FlexLayout>
        </StackLayout>
    </ContentView.Content>
</ContentView>